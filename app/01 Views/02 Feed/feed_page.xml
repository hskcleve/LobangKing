<Page id="feed_page" loaded="onLoaded" navigatedTo="onNavigatedTo">
    <GridLayout class="gradientBg" rows="auto,*,auto">

        <GridLayout height="10%" class="topBar" row="0" columns="20,*">
            <Label col="1" class="h1" text="{{'Welcome, ' + user.first_name + ' ' + user.last_name}}"/>
        </GridLayout>

        <StackLayout row="1">
            <Button class="-primary" text="Back to Login" tap="logoutOnTap"/>
            <Button class="-primary" text="Temp Link to Profile" tap="profileOnTap"/>
        </StackLayout>
        
        <GridLayout row="3" style="background-color:grey" height="7%">
            <Label style="color:white; text-align:center" text="<unimplemented navbar>"/>
        </GridLayout>

    </GridLayout>
</Page>